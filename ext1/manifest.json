{
  "manifest_version": 3,
  "name": "OS Keyboard Shortcuts",
  "version": "1.0",
  "description": "Choose your OS and view the corresponding keyboard shortcuts.",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }

  
}

  